mpirun -np 16 HPCCG-MIX 90 90 50 5 > baseline-16rank.txt            &&
mpirun -np 32 HPCCG-MIX 90 90 25 5 > baseline-32rank.txt            

-- 1 rank tests
mpirun -np 1 HPCCG-WANG 90 90 800 5 2 0 2 > replicated-wang-1rank-noHT.txt   &&
mpirun -np 1 HPCCG-WANG 90 90 800 5 2 0 16 > replicated-wang-1rank-HT.txt  &&
mpirun -np 1 HPCCG-WANG-VG 90 90 800 5 2 0 2 > replicated-wang-vg-1rank-noHT.txt   &&
mpirun -np 1 HPCCG-WANG-VG 90 90 800 5 2 0 16 > replicated-wang-vg-1rank-HT.txt
    
-- all resources tests
mpirun -np 16 HPCCG-WANG 90 90 50 5 32 0 2 4 6 8 10 12 14 1 3 5 7 9 11 13 15 16 18 20 22 24 26 28 30 17 19 21 23 25 27 29 31 > replicated-wang-16rank-noHT.txt            &&
